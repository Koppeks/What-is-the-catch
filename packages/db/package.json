{
  "name": "@repo/db",
  "private": true,
  "version": "0.0.9",
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": ">=20.11 <21"
  },
  "scripts": {
    "prisma:migrate": "dotenv -e ../../.env -- prisma migrate dev",
    "prisma:reset": "dotenv -e ../../.env -- prisma migrate reset --force --skip-seed",
    "prisma:generate": "dotenv -e ../../.env -- prisma generate",
    "prisma:studio": "dotenv -e ../../.env -- prisma studio",
    "prisma:seed": "dotenv -e ../../.env -- tsx ./prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "*",
    "prisma": "*",
    "zod": "*",
    "zod-prisma-types": "*"
  },
  "devDependencies": {
    "dotenv-cli": "*",
    "tsx": "*",
    "typescript": "*"
  }
}

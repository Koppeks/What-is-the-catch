{
  "name": "@repo/db",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": ">=20.11 <21"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "prisma:migrate": "dotenv -e ../../.env -- prisma migrate dev",
    "prisma:reset": "dotenv -e ../../.env -- prisma migrate reset --force --skip-seed --schema ./prisma/schema.prisma",
    "prisma:generate": "dotenv -e ../../.env -- prisma generate",
    "prisma:studio": "dotenv -e ../../.env -- prisma studio",
    "prisma:seed": "dotenv -e ../../.env -- tsx ./prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "*",
    "prisma": "*",
    "zod": "*",
    "zod-prisma-types": "*"
  },
  "devDependencies": {
    "tsx": "*",
    "typescript": "*"
  }
}
